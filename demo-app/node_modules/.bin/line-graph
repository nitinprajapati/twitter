#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/electro" ]; then
  "$basedir/electro"  "$basedir/../line-graph/bin.js" "$@"
  ret=$?
else 
  electro  "$basedir/../line-graph/bin.js" "$@"
  ret=$?
fi
exit $ret
